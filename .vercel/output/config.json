{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api(?:/(.*))$",
      "dest": "/api/$1",
      "check": true
    },
    {
      "src": "^(?:/((?!api/).*))$",
      "dest": "/index.html",
      "check": true
    },
    {
      "src": "^/api/book$",
      "dest": "/api/book",
      "check": true
    }
  ],
  "overrides": {}
}